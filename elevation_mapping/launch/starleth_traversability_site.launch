<launch>
	<env name="ROSCONSOLE_CONFIG_FILE" value="$(find elevation_mapping)/launch/custom_rosconsole.conf"/>
 	<node pkg="elevation_mapping" type="elevation_mapping" name="elevation_mapping_long_range" output="screen">
    	 <rosparam command="load" file="$(find elevation_mapping)/config/robots/starleth_long_range.yaml" />
    	 <rosparam command="load" file="$(find elevation_mapping)/config/elevation_maps/ground_truth.yaml" />
    	 <rosparam command="load" file="$(find elevation_mapping)/config/sensor_processors/hokuyo_utm-30lx.yaml" />
    	 <param name="path_to_bag" value="$(find traversability_estimation)/data/elevationMap.bag" />
    </node>
</launch>